<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Bug Tracker - @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @*<link href="~/Content/bootstrap-multiselect.css" rel="stylesheet" />*@
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/teal.css" rel="stylesheet" />
    <link href="~/Content/invalid.css" rel="stylesheet" />
    <link href="~/Content/multi-select.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
        @RenderBody()
    @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
    <!--=========Preload script for css and images=========-->
    <script src="~/Scripts/js/preloadCssImages.jQuery_v5.js"></script>
    <!--=========Scripts=========-->
    <script src="~/Scripts/js/script.js"></script>
    <!--=========Date picker=========-->
    <script src="~/Scripts/js/jCal.js"></script>
    <script src="~/Scripts/js/jquery.animate.clip.js"></script>

    <!--=========Visualize for charting=========-->
    <script src="~/Scripts/js/visualize.jQuery.js"></script>
    <!--[if IE]><script language="javascript" type="text/javascript" src="~/Scripts/js/excanvas.min.js"></script><![endif]-->
    <!--=========wysiwyg editor=========-->
    <script src="~/Scripts/js/jquery.wysiwyg.js"></script>
    <script src="~/Scripts/jquery.multi-select.js"></script>

    <!--=========jExpand for Expanding Tables=========-->
    <script src="~/Scripts/js/jExpand.js"></script>

    <!--=========Data Tables=========-->
    <script src="~/Scripts/js/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/js/TableTools.min.js"></script>
    <script src="~/Scripts/js/ZeroClipboard.js"></script>
    @*<script src="~/Scripts/bootstrap-multiselect.js"></script>*@
    @RenderSection("scripts", required: false)
</body>
</html>
